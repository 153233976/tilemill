<div class='projects'>
  <% if (!models.length) { %>
  <div class='empty'>No projects found.</div>
  <% } %>

  <ul>
    <% _(models).each(function(model) { %><li>
      <a class='thumb' href='/#!/project/<%= model.id %>'>
        <span class='thumb-layer fill-w' style='background-image: url(<%= model.thumb() %>)'></span>
        <span class='thumb-name fill-e'>
          <%= model.get('name') || model.get('id') %>
          <span id='<%= model.id %>' class='icon delete'>Delete</span>
        </span>
      </a>
    </li><% }); %>
  </ul>
</div>
