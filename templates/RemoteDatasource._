<% if (typeof type !== 'undefined' && type == 'index') { %>
<ul>
<% _(items).each(function(i) { %>
<a href='<%=i.link%>' class='remote-item'><strong><%=i.name %></strong> - <small><%=i.description %></small> / z<%=i.minzoom%>-<%=i.maxzoom%></a>
<% }); %>
</ul>
<% } else { %>
<strong><%=name %></strong> - <small><%=description %></small> / z<%=minzoom%>-<%=maxzoom%>
<% } %>
