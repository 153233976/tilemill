<div class='project'>
  <div class='bleed'><div class='header'>
    <div class='breadcrumb'>
      <a class='logo' href='/#!/'><span class='icon'></span> TileMill</a>
      <a href='/#!/project/<%= id %>'><%= id %></a>
    </div>
    <div class='actions'>
      <a class='button' href='#save'>Save</a>
      <span class='button dropdown' href='#export'>
        <span class='icon reverse right dropdown'></span>
        Export
        <span class='menu'>
          <a href='#png'>PNG</a>
          <a href='#pdf'>PDF</a>
          <a href='#mbtiles'>MBTiles</a>
        </span>
      </span>
      <a class='popup button' href='#settings'>
        <span class='icon reverse settings'>Settings</span>
      </a>
    </div>
  </div></div>
  <div class='editor fill-e'>
    <div class='code fill-s'></div>

    <div class='palette clearfix'>
      <div class='colors'>
        <a class='button' href='#colorpicker'><span class='icon reverse color-picker'>Colorpicker</span></a>
      </div>
      <div class='ref'>
        <a class='drawer button' href='#fonts'><span class='icon reverse fonts'>Fonts</span></a>
        <a class='drawer button' href='#carto'><span class='icon reverse reference'>Carto</span></a>
      </div>
    </div>

    <ul class='tabs toggler clearfix'></ul>
    <a class='popup add button'><span class='icon reverse plus'>Add stylesheet</span></a>
  </div>
  <div class='map fill-w' id='map'>
    <div class='zoom-display'>Zoom <span class='zoom'></span></div>
  </div>
  <div class='layers'>
    <a href='#add' class='popup add button'><span class='icon reverse plus'></span> Add layer</a>
    <ul></ul>
  </div>
  <div id='drawer' class='pane fill-w'>
    <h2 class='title'></h2>
    <a href='#close' class='close button'><span class='icon reverse close'>Close</span></a>
    <div class='content fill-s'></div>
  </div>
</div>
