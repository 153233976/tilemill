<div class='fonts'>

<div class='links fill-n'>
  <a class='active all' href='#0'>All</a>
  <% _(65).chain().range(91).each(function(code) { %>
    <% if (groups[code]) { %>
      <a href='#<%= code %>'><%= String.fromCharCode(code) %></a>
    <% } else { %>
      <span><%= String.fromCharCode(code) %></span>
    <% } %>
  <% }); %>
</div>

<% _(fonts).each(function(f) { %>
<a href='#insert' class='font group-0 group-<%= f.group %>'><%= f.name %></a>
<% }); %>
</div>
