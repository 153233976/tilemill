<table class='features'>
  <thead>
    <tr>
      <% _(fields).each(function (field, name) { %>
        <th><a title='<%= field.type %>'><%= name %></a></th>
      <% }); %>
    </tr>
  </thead>
  <tbody>
    <%= this['DatasourceInfoRows'](obj) %>
  </tbody>
</table>
<% if (more) { %>
<a class='showall button' href='#'>Show all</a>
<% } %>
